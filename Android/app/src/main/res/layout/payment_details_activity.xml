			android:layout_marginLeft="@dimen/payment_details_activity_number_constraint_layout_margin_start"
			android:layout_marginTop="@dimen/payment_details_activity_number_constraint_layout_margin_top"
			android:background="@drawable/payment_details_activity_number_constraint_layout_background"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/owner_constraint_layout"
			tools:layout_editor_absoluteX="1dp"
			tools:layout_editor_absoluteY="130dp">

			<EditText
				android:id="@+id/card_number_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginStart="8dp"
				android:background="@android:color/transparent"
				android:fontFamily="@font/font_avenir_medium"
				android:gravity="start"
				android:inputType="number"
				android:lineSpacingMultiplier="1.17"
				android:text="@string/payment_details_activity_card_number_text_view_text"
				android:textColor="@color/payment_details_activity_card_number_text_view_text_color"
				android:textColorHint="@color/payment_details_activity_card_number_text_view_text_color"
				android:textSize="@dimen/payment_details_activity_card_number_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintStart_toEndOf="@+id/icon24px_three_image_view"
				app:layout_constraintTop_toTopOf="parent" />

			<ImageView
				android:id="@+id/icon24px_three_image_view"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/payment_details_activity_icon24px_three_image_view_margin_start"
				android:scaleType="center"
				android:src="@drawable/icon_24px_2"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent" />
		</android.support.constraint.ConstraintLayout>

		<android.support.constraint.ConstraintLayout
			android:id="@+id/owner_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/payment_details_activity_owner_constraint_layout_height"
			android:layout_marginLeft="@dimen/payment_details_activity_owner_constraint_layout_margin_start"
			android:layout_marginTop="@dimen/payment_details_activity_owner_constraint_layout_margin_top"
			android:background="@drawable/payment_details_activity_owner_constraint_layout_background"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/title_text_view"
			tools:layout_editor_absoluteX="1dp"
			tools:layout_editor_absoluteY="55dp">

			<EditText
				android:id="@+id/card_owner_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/payment_details_activity_card_owner_text_view_margin_start"
				android:background="@android:color/transparent"
				android:fontFamily="@font/font_avenir_medium"
				android:gravity="start"
				android:hint="Card Owner"
				android:inputType="textPersonName"
				android:lineSpacingMultiplier="1.17"
				android:text="@string/payment_details_activity_card_owner_text_view_text"
				android:textColor="@color/payment_details_activity_card_owner_text_view_text_color"
				android:textColorHint="@color/payment_details_activity_card_number_text_view_text_color"
				android:textColorLink="@color/payment_details_activity_card_number_text_view_text_color"
				android:textSize="@dimen/payment_details_activity_card_owner_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toRightOf="@+id/person_material_image_view"
				app:layout_constraintTop_toTopOf="parent" />

			<ImageView
				android:id="@+id/person_material_image_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/payment_details_activity_person_material_image_view_margin_start"
				android:scaleType="center"
				android:src="@drawable/person___material"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="25dp"
				tools:layout_editor_absoluteY="20dp"/>
		</android.support.constraint.ConstraintLayout>

		<TextView
			android:id="@+id/title_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="@dimen/payment_details_activity_title_text_view_margin_top"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="start"
			android:lineSpacingMultiplier="1.16"
			android:text="@string/payment_details_activity_title_text_view_text"
			android:textColor="@color/payment_details_activity_title_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_title_text_view_text_size"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="-2dp"/>
	</android.support.constraint.ConstraintLayout>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/card_constraint_layout"
		android:layout_width="@dimen/payment_details_activity_card_constraint_layout_width"
		android:layout_height="@dimen/payment_details_activity_card_constraint_layout_height"
		android:layout_marginTop="@dimen/payment_details_activity_card_constraint_layout_margin_top"
		android:background="@drawable/payment_details_activity_card_constraint_layout_background"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/payment_constraint_layout"
		tools:layout_editor_absoluteX="25dp"
		tools:layout_editor_absoluteY="75dp">

		<TextView
			android:id="@+id/name_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="@dimen/payment_details_activity_name_text_view_margin_bottom"
			android:layout_marginLeft="@dimen/payment_details_activity_name_text_view_margin_start"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="start"
			android:lineSpacingMultiplier="1.17"
			android:text="@string/payment_details_activity_name_text_view_text"
			android:textColor="@color/payment_details_activity_name_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_name_text_view_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			tools:layout_editor_absoluteX="24dp"
			tools:layout_editor_absoluteY="130dp"/>

		<TextView
			android:id="@+id/card_owner_two_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/payment_details_activity_card_owner_two_text_view_margin_start"
			android:layout_marginTop="@dimen/payment_details_activity_card_owner_two_text_view_margin_top"
			android:fontFamily="@font/font_avenir_light"
			android:gravity="start"
			android:lineSpacingMultiplier="1.14"
			android:text="@string/payment_details_activity_card_owner_two_text_view_text"
			android:textColor="@color/payment_details_activity_card_owner_two_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_card_owner_two_text_view_text_size"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/number_text_view"
			tools:layout_editor_absoluteX="25dp"
			tools:layout_editor_absoluteY="112dp"/>

		<TextView
			android:id="@+id/month_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="@dimen/payment_details_activity_month_text_view_margin_bottom"
			android:layout_marginRight="@dimen/payment_details_activity_month_text_view_margin_end"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="start"
			android:lineSpacingMultiplier="1.17"
			android:text="@string/payment_details_activity_month_text_view_text"
			android:textColor="@color/payment_details_activity_month_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_month_text_view_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintRight_toLeftOf="@+id/month_two_text_view"
			tools:layout_editor_absoluteX="200dp"
			tools:layout_editor_absoluteY="130dp"/>

		<TextView
			android:id="@+id/issued_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="@dimen/payment_details_activity_issued_text_view_margin_end"
			android:layout_marginTop="@dimen/payment_details_activity_issued_text_view_margin_top"
			android:fontFamily="@font/font_avenir_light"
			android:gravity="start"
			android:lineSpacingMultiplier="1.14"
			android:text="@string/payment_details_activity_issued_text_view_text"
			android:textColor="@color/payment_details_activity_issued_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_issued_text_view_text_size"
			app:layout_constraintRight_toLeftOf="@+id/expire_text_view"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="199dp"
			tools:layout_editor_absoluteY="112dp"/>

		<TextView
			android:id="@+id/month_two_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="@dimen/payment_details_activity_month_two_text_view_margin_bottom"
			android:layout_marginRight="@dimen/payment_details_activity_month_two_text_view_margin_end"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="start"
			android:lineSpacingMultiplier="1.17"
			android:text="@string/payment_details_activity_month_two_text_view_text"
			android:textColor="@color/payment_details_activity_month_two_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_month_two_text_view_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_editor_absoluteX="263dp"
			tools:layout_editor_absoluteY="130dp"/>

		<TextView
			android:id="@+id/expire_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="@dimen/payment_details_activity_expire_text_view_margin_end"
			android:layout_marginTop="@dimen/payment_details_activity_expire_text_view_margin_top"
			android:fontFamily="@font/font_avenir_light"
			android:gravity="start"
			android:lineSpacingMultiplier="1.14"
			android:text="@string/payment_details_activity_expire_text_view_text"
			android:textColor="@color/payment_details_activity_expire_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_expire_text_view_text_size"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/visa_image_view"
			tools:layout_editor_absoluteX="263dp"
			tools:layout_editor_absoluteY="112dp"/>

		<TextView
			android:id="@+id/number_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/payment_details_activity_number_text_view_margin_start"
			android:layout_marginTop="@dimen/payment_details_activity_number_text_view_margin_top"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="start"
			android:lineSpacingMultiplier="1.17"
			android:text="@string/payment_details_activity_number_text_view_text"
			android:textColor="@color/payment_details_activity_number_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_number_text_view_text_size"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/card_number_two_text_view"
			tools:layout_editor_absoluteX="24dp"
			tools:layout_editor_absoluteY="55dp"/>

		<TextView
			android:id="@+id/card_number_two_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/payment_details_activity_card_number_two_text_view_margin_start"
			android:layout_marginTop="@dimen/payment_details_activity_card_number_two_text_view_margin_top"
			android:fontFamily="@font/font_avenir_light"
			android:gravity="start"
			android:lineSpacingMultiplier="1.14"
			android:text="@string/payment_details_activity_card_number_two_text_view_text"
			android:textColor="@color/payment_details_activity_card_number_two_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_card_number_two_text_view_text_size"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="25dp"
			tools:layout_editor_absoluteY="37dp"/>

		<ImageView
			android:id="@+id/visa_image_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="@dimen/payment_details_activity_visa_image_view_margin_end"
			android:layout_marginTop="@dimen/payment_details_activity_visa_image_view_margin_top"
			android:scaleType="center"
			android:src="@drawable/visa"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="265dp"
			tools:layout_editor_absoluteY="22dp"/>
	</android.support.constraint.ConstraintLayout>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/payment_constraint_layout"
		android:layout_width="@dimen/payment_details_activity_payment_constraint_layout_width"
		android:layout_height="@dimen/payment_details_activity_payment_constraint_layout_height"
		android:layout_marginTop="@dimen/payment_details_activity_payment_constraint_layout_margin_top"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="25dp"
		tools:layout_editor_absoluteY="21dp">

		<TextView
			android:id="@+id/payment_details_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="center"
			android:text="@string/payment_details_activity_payment_details_text_view_text"
			android:textColor="@color/payment_details_activity_payment_details_text_view_text_color"
			android:textSize="@dimen/payment_details_activity_payment_details_text_view_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="96dp"
			tools:layout_editor_absoluteY="0dp"/>

		<ImageButton
			android:id="@+id/header_menu_dark_button"
			android:layout_width="@dimen/payment_details_activity_header_menu_dark_button_width"
			android:layout_height="@dimen/payment_details_activity_header_menu_dark_button_height"
			android:background="?attr/selectableItemBackground"
			android:src="@drawable/header_menu_dark"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="4dp"/>
	</android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>