		android:layout_height="@dimen/payment_details_activity_add_new_form_constraint_layout_height"
		android:layout_marginTop="@dimen/payment_details_activity_add_new_form_constraint_layout_margin_top"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/card_constraint_layout"
		tools:layout_editor_absoluteX="23dp"
		tools:layout_editor_absoluteY="295dp">

		<android.support.constraint.ConstraintLayout
			android:id="@+id/button_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/payment_details_activity_button_constraint_layout_height"
			android:layout_marginLeft="@dimen/payment_details_activity_button_constraint_layout_margin_start"
			android:background="@drawable/payment_details_activity_button_constraint_layout_background"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_editor_absoluteX="1dp"
			tools:layout_editor_absoluteY="280dp">

			<TextView
				android:id="@+id/add_new_card_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:fontFamily="@font/font_avenir_medium"
				android:gravity="center"
				android:lineSpacingMultiplier="0.73"
				android:text="@string/payment_details_activity_add_new_card_text_view_text"
				android:textColor="@color/payment_details_activity_add_new_card_text_view_text_color"
				android:textSize="@dimen/payment_details_activity_add_new_card_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="99dp"
				tools:layout_editor_absoluteY="14dp"/>
		</android.support.constraint.ConstraintLayout>

		<android.support.constraint.ConstraintLayout
			android:id="@+id/month_constraint_layout"
			android:layout_width="@dimen/payment_details_activity_month_constraint_layout_width"
			android:layout_height="@dimen/payment_details_activity_month_constraint_layout_height"
			android:layout_marginLeft="@dimen/payment_details_activity_month_constraint_layout_margin_start"
			android:layout_marginTop="@dimen/payment_details_activity_month_constraint_layout_margin_top"
			android:background="@drawable/payment_details_activity_month_constraint_layout_background"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/number_constraint_layout"
			tools:layout_editor_absoluteX="1dp"
			tools:layout_editor_absoluteY="205dp">

			<EditText
				android:id="@+id/exp_month_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/payment_details_activity_exp_month_text_view_margin_end"
				android:background="@android:color/transparent"
				android:fontFamily="@font/font_avenir_medium"
				android:gravity="start"
				android:hint="Exp. Month"
				android:inputType="number"
				android:lineSpacingMultiplier="1.17"
				android:text="@string/payment_details_activity_exp_month_text_view_text"
				android:textColor="@color/payment_details_activity_exp_month_text_view_text_color"
				android:textColorHint="@color/payment_details_activity_card_number_text_view_text_color"
				android:textSize="@dimen/payment_details_activity_exp_month_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent" />

			<ImageView
				android:id="@+id/icon24px_image_view"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/payment_details_activity_icon24px_image_view_margin_start"
				android:layout_marginRight="@dimen/payment_details_activity_icon24px_image_view_margin_end"
				android:scaleType="center"
				android:src="@drawable/icon_24px_13"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toLeftOf="@+id/exp_month_text_view"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="24dp"
				tools:layout_editor_absoluteY="19dp"/>
		</android.support.constraint.ConstraintLayout>

		<android.support.constraint.ConstraintLayout
			android:id="@+id/year_constraint_layout"
			android:layout_width="@dimen/payment_details_activity_year_constraint_layout_width"
			android:layout_height="@dimen/payment_details_activity_year_constraint_layout_height"
			android:layout_marginTop="@dimen/payment_details_activity_year_constraint_layout_margin_top"
			android:background="@drawable/payment_details_activity_year_constraint_layout_background"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/number_constraint_layout"
			tools:layout_editor_absoluteX="176dp"
			tools:layout_editor_absoluteY="205dp">

			<EditText
				android:id="@+id/exp_year_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@android:color/transparent"
				android:fontFamily="@font/font_avenir_medium"
				android:gravity="start"
				android:hint="Exp. Year"
				android:inputType="number"
				android:lineSpacingMultiplier="1.17"
				android:text="@string/payment_details_activity_exp_year_text_view_text"
				android:textColor="@color/payment_details_activity_exp_year_text_view_text_color"
				android:textColorHint="@color/payment_details_activity_card_number_text_view_text_color"
				android:textSize="@dimen/payment_details_activity_exp_year_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent" />

			<ImageView
				android:id="@+id/icon24px_two_image_view"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="@dimen/payment_details_activity_icon24px_two_image_view_margin_start"
				android:layout_marginRight="@dimen/payment_details_activity_icon24px_two_image_view_margin_end"
				android:scaleType="center"
				android:src="@drawable/icon_24px_13"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toLeftOf="@+id/exp_year_text_view"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="24dp"
				tools:layout_editor_absoluteY="19dp"/>
		</android.support.constraint.ConstraintLayout>

		<android.support.constraint.ConstraintLayout
