<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/north_spine_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/north_spine_two_activity_north_spine_constraint_layout_background_color">

	<LinearLayout
		android:id="@+id/ricestall_button"
		android:layout_width="@dimen/north_spine_two_activity_ricestall_button_width"
		android:layout_height="@dimen/north_spine_two_activity_ricestall_button_height"
		android:layout_marginTop="@dimen/north_spine_two_activity_ricestall_button_margin_top"
		android:background="?attr/selectableItemBackground"
		android:gravity="center"
		android:orientation="horizontal"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/northspinerestaurents_constraint_layout"
		tools:layout_editor_absoluteX="24dp"
		tools:layout_editor_absoluteY="116dp"
		style="@style/Widget.AppCompat.Button">

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/bitmap_2" />
	
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/north_spine_two_activity_ricestall_button_image_padding"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="start"
			android:text="@string/north_spine_two_activity_ricestall_button_text"
			android:textColor="@color/north_spine_two_activity_ricestall_button_text_color"
			android:textSize="@dimen/north_spine_two_activity_ricestall_button_text_size"/>
	</LinearLayout>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/northspinerestaurents_constraint_layout"
		android:layout_width="0dp"
		android:layout_height="22dp"
		android:layout_marginStart="32dp"
		android:layout_marginTop="@dimen/north_spine_two_activity_northspinerestaurents_constraint_layout_margin_top"
		android:layout_marginEnd="32dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent">

		<TextView
			android:id="@+id/north_spine_food_cou_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="8dp"
			android:layout_marginEnd="8dp"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="center"
			android:text="Singaporean"
			android:textColor="@color/north_spine_two_activity_north_spine_food_cou_text_view_text_color"
			android:textSize="@dimen/north_spine_two_activity_north_spine_food_cou_text_view_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/cart_image_view3"
			app:layout_constraintStart_toEndOf="@+id/header_menu_dark_image_view"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintVertical_bias="0.0" />

		<ImageView
			android:id="@+id/header_menu_dark_image_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/header_menu_dark"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<ImageView
			android:id="@+id/cart_image_view3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/ic_baseline_local_grocery_store_24px"
			android:tint="@color/cart_activity_cart_text_view_text_color"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent" />
	</android.support.constraint.ConstraintLayout>

	<LinearLayout
		android:id="@+id/vegetablestall_button"
		android:layout_width="@dimen/north_spine_two_activity_vegetablestall_button_width"
		android:layout_height="@dimen/north_spine_two_activity_vegetablestall_button_height"
		android:layout_marginTop="@dimen/north_spine_two_activity_vegetablestall_button_margin_top"
		android:background="?attr/selectableItemBackground"
		android:gravity="center"
		android:orientation="horizontal"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/ricestall_button"
		tools:layout_editor_absoluteX="25dp"
		tools:layout_editor_absoluteY="291dp"
		style="@style/Widget.AppCompat.Button">

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="14dp"
			android:src="@drawable/bitmap_5" />
	
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/north_spine_two_activity_vegetablestall_button_image_padding"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="start"
			android:text="@string/north_spine_two_activity_vegetablestall_button_text"
			android:textColor="@color/north_spine_two_activity_vegetablestall_button_text_color"
			android:textSize="@dimen/north_spine_two_activity_vegetablestall_button_text_size"/>
	</LinearLayout>
</android.support.constraint.ConstraintLayout>