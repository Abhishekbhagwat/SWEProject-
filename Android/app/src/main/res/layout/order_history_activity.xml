<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/order_history_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/order_history_activity_order_history_constraint_layout_background_color">

	<android.support.constraint.ConstraintLayout
		android:id="@+id/group3_constraint_layout"
		android:layout_width="@dimen/order_history_activity_group3_constraint_layout_width"
		android:layout_height="@dimen/order_history_activity_group3_constraint_layout_height"
		android:layout_marginTop="@dimen/order_history_activity_group3_constraint_layout_margin_top"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/orderhistory_constraint_layout"
		tools:layout_editor_absoluteX="24dp"
		tools:layout_editor_absoluteY="116dp">
	
		<android.support.constraint.ConstraintLayout
			android:id="@+id/category_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/order_history_activity_category_constraint_layout_height"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="0dp">
		
			<TextView
				android:id="@+id/title_text_view"
				android:layout_width="@dimen/order_history_activity_title_text_view_width"
				android:layout_height="wrap_content"
				android:fontFamily="@font/font_avenir_medium"
				android:gravity="start"
				android:lineSpacingMultiplier="1.17"
				android:text="@string/order_history_activity_title_text_view_text"
				android:textColor="@color/order_history_activity_title_text_view_text_color"
				android:textSize="@dimen/order_history_activity_title_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="212dp"
				tools:layout_editor_absoluteY="14dp"/>
		
			<ImageView
				android:id="@+id/bitmap_image_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:scaleType="center"
				android:src="@drawable/bitmap_2"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="0dp"
				tools:layout_editor_absoluteY="0dp"/>
		</android.support.constraint.ConstraintLayout>
	
		<View
			android:id="@+id/line_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/order_history_activity_line_constraint_layout_height"
			android:background="@color/order_history_activity_line_constraint_layout_background_color"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="154dp"/>
	</android.support.constraint.ConstraintLayout>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/orderhistory_constraint_layout"
		android:layout_width="@dimen/order_history_activity_orderhistory_constraint_layout_width"
		android:layout_height="@dimen/order_history_activity_orderhistory_constraint_layout_height"
		android:layout_marginLeft="@dimen/order_history_activity_orderhistory_constraint_layout_margin_start"
		android:layout_marginTop="@dimen/order_history_activity_orderhistory_constraint_layout_margin_top"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="25dp"
		tools:layout_editor_absoluteY="21dp">
	
		<TextView
			android:id="@+id/order_history_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:fontFamily="@font/font_avenir_medium"
			android:gravity="center"
			android:text="@string/order_history_activity_order_history_text_view_text"
			android:textColor="@color/order_history_activity_order_history_text_view_text_color"
			android:textSize="@dimen/order_history_activity_order_history_text_view_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="103dp"
			tools:layout_editor_absoluteY="0dp"/>
	
		<ImageView
			android:id="@+id/header_menu_dark_image_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:scaleType="center"
			android:src="@drawable/header_menu_dark"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="4dp"/>
	</android.support.constraint.ConstraintLayout>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/group3_two_constraint_layout"
		android:layout_width="@dimen/order_history_activity_group3_two_constraint_layout_width"
		android:layout_height="@dimen/order_history_activity_group3_two_constraint_layout_height"
		android:layout_marginTop="@dimen/order_history_activity_group3_two_constraint_layout_margin_top"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/group3_constraint_layout"
		tools:layout_editor_absoluteX="25dp"
		tools:layout_editor_absoluteY="291dp">
	
		<android.support.constraint.ConstraintLayout
			android:id="@+id/category_two_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/order_history_activity_category_two_constraint_layout_height"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="0dp">
		
			<TextView
				android:id="@+id/title_two_text_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="@dimen/order_history_activity_title_two_text_view_margin_end"
				android:fontFamily="@font/font_avenir_medium"
				android:gravity="start"
				android:lineSpacingMultiplier="1.17"
				android:text="@string/order_history_activity_title_two_text_view_text"
				android:textColor="@color/order_history_activity_title_two_text_view_text_color"
				android:textSize="@dimen/order_history_activity_title_two_text_view_text_size"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="212dp"
				tools:layout_editor_absoluteY="14dp"/>
		
			<ImageView
				android:id="@+id/bitmap_two_image_view"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:scaleType="center"
				android:src="@drawable/bitmap_5"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="0dp"
				tools:layout_editor_absoluteY="0dp"/>
		</android.support.constraint.ConstraintLayout>
	
		<View
			android:id="@+id/line_two_constraint_layout"
			android:layout_width="0dp"
			android:layout_height="@dimen/order_history_activity_line_two_constraint_layout_height"
			android:background="@color/order_history_activity_line_two_constraint_layout_background_color"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="154dp"/>
	</android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>